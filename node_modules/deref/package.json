    "test": "npm run dev:lint && npm run dev:spec"
  },
  "devDependencies": {
    "clone": "^2.1.1",
    "codecov": "^3.0.0",
    "eslint": "^4.14.0",
    "glob": "^7.1.2",
    "is-my-json-valid": "^2.13.1",
    "istanbul": "^0.4.2",
    "jasmine-node": "2.0.0-beta4",
    "jayschema": "^0.3.1",
    "tv4": "^1.2.7",
    "z-schema": "^3.15.4"
  },
  "dependencies": {
    "deep-extend": "^0.5.0"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org"
  }
